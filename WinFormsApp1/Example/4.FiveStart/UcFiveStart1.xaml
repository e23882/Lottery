                                    <Button Tag="Type2" Content="2" Width="20" Click="Button_Click"/>
                                    <Button Tag="Type2" Content="3" Width="20" Click="Button_Click"/>
                                    <Button Tag="Type2" Content="4" Width="20" Click="Button_Click"/>
