                                    <Button Tag="Unit3" Content="1" Width="20" Click="Button_Click"/>
                                    <Button Tag="Unit3" Content="2" Width="20" Click="Button_Click"/>
                                    <Button Tag="Unit3" Content="3" Width="20" Click="Button_Click"/>
