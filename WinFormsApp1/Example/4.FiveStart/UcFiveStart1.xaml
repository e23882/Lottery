                                </DockPanel>
                            </DockPanel>
                            <DockPanel DockPanel.Dock="Bottom" x:Name="dpType4">
                                <DockPanel DockPanel.Dock="Left" >
                                    <Button Tag="Type4" Content="0" Width="20" Click="Button_Click"/>
                                    <Button Tag="Type4" Content="1" Width="20" Click="Button_Click"/>
