                            <Button Tag="Clear4" Content="清" Click="Button_Click" Width="28"/>
                        </DockPanel>
                    </DockPanel>

                    <!--使用CheckBoxList計算值-->
                    <DockPanel Visibility="Collapsed">
                        <DockPanel DockPanel.Dock="Top">
                            <local:CheckBoxList x:Name="cblNumber1" DisplayMemberPath="Name" ValueMemberPath="ID" DockPanel.Dock="Left"/>
                            <local:CheckBoxList x:Name="cblNumber1_2" DisplayMemberPath="Name" ValueMemberPath="ID" DockPanel.Dock="Right"/>
                        </DockPanel>
                        <DockPanel DockPanel.Dock="Top">
                            <local:CheckBoxList x:Name="cblNumber2" DisplayMemberPath="Name" ValueMemberPath="ID" DockPanel.Dock="Left" />
                            <local:CheckBoxList x:Name="cblNumber2_2" DisplayMemberPath="Name" ValueMemberPath="ID" DockPanel.Dock="Right"/>
                        </DockPanel>
                        <DockPanel DockPanel.Dock="Top">
                            <local:CheckBoxList x:Name="cblNumber3" DisplayMemberPath="Name" ValueMemberPath="ID" DockPanel.Dock="Left"/>
                            <local:CheckBoxList x:Name="cblNumber3_2" DisplayMemberPath="Name" ValueMemberPath="ID" DockPanel.Dock="Right"/>
                        </DockPanel>
                        <DockPanel DockPanel.Dock="Bottom">
                            <local:CheckBoxList x:Name="cblNumber4" DisplayMemberPath="Name" ValueMemberPath="ID" DockPanel.Dock="Left"/>
                            <local:CheckBoxList x:Name="cblNumber4_2" DisplayMemberPath="Name" ValueMemberPath="ID" DockPanel.Dock="Right"/>
                        </DockPanel>
                    </DockPanel>
                    <DockPanel DockPanel.Dock="Bottom">
                        <TextBlock Text="胆组容错" Margin="0,7,5,0" DockPanel.Dock="Left"/>
                        <TextBox Text="0" x:Name="teStart" Width="20" TextChanged="te_TextChanged" Margin="0,5,5,0" DockPanel.Dock="Left"/>
                        <TextBlock Text="至" Margin="0,7,5,0" DockPanel.Dock="Left"/>
                        <TextBox Text="0" x:Name="teEnd" Width="20" TextChanged="te_TextChanged" Margin="0,5,5,0" DockPanel.Dock="Left" HorizontalAlignment="Left"/>
                    </DockPanel>
                </DockPanel>
            </GroupBox>

            <GroupBox Header="杀特殊形态" Grid.Row="4" >
                <local:CheckBoxList x:Name="cblSpecial" DisplayMemberPath="Name" ValueMemberPath="ID"/>
            </GroupBox>
        </Grid>

        <!--右半-->
        <Grid Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="180"/>
                <ColumnDefinition Width="180"/>
                <ColumnDefinition Width="180"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>

            <GroupBox Header="杀大小排列" Margin="0,0,0,3" Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" >
                <local:CheckBoxList Margin="5" x:Name="cblData1" DisplayMemberPath="Name" ValueMemberPath="ID" />
            </GroupBox>

            <GroupBox Header="杀奇偶排列" Margin="0,0,0,3" Grid.Row="0" Grid.Column="1" Grid.RowSpan="2" >
                <local:CheckBoxList Margin="5" x:Name="cblData2" DisplayMemberPath="Name" ValueMemberPath="ID" />
            </GroupBox>

            <GroupBox Header="杀质合排列" Margin="0,0,0,3" Grid.Row="0" Grid.Column="2" Grid.RowSpan="2" >
                <local:CheckBoxList Margin="5" x:Name="cblData3" DisplayMemberPath="Name" ValueMemberPath="ID" />
            </GroupBox>

            <GroupBox Header="杀012排列" Margin="0,0,0,3" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3" Grid.RowSpan="2">
                <DockPanel Width="550">
                    <local:CheckBoxList x:Name="cbl012" Margin="5" DisplayMemberPath="Name" ValueMemberPath="ID" />
                </DockPanel>
            </GroupBox>

            <TextBox x:Name="teNumber" Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3" MouseDoubleClick="txtEditor_MouseDoubleClick">
                <TextBox.Resources>
                    <VisualBrush x:Key="HintText" TileMode="None" Opacity="0.5" Stretch="None" AlignmentX="Left" AlignmentY="Top">
                        <VisualBrush.Visual>
                            <TextBlock FontStyle="Normal" Text="大底号码 双击清空"/>
                        </VisualBrush.Visual>
                    </VisualBrush>
                </TextBox.Resources>
                <TextBox.Style>
                    <Style TargetType="TextBox">
                        <Style.Triggers>
                            <Trigger Property="Text" Value="{x:Null}">
                                <Setter Property="Background" Value="{StaticResource HintText}"/>
                            </Trigger>
                            <Trigger Property="Text" Value="">
                                <Setter Property="Background" Value="{StaticResource HintText}"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </TextBox.Style>
            </TextBox>

            <!--<DockPanel Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3">
                
            </DockPanel>-->

            <GroupBox Header="特别排除" Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="3">
                <local:CheckBoxList x:Name="cblSpecialExcept" DisplayMemberPath="Name" ValueMemberPath="ID"/>
            </GroupBox>
        </Grid>
    </Grid>
</UserControl>
